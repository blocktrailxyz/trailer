{
  "name": "trailer",
  "version": "1.0.0",
  "description": "Trailer run on nodejs23.1.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:debug": "NODE_ENV=test node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "lint:fix": "eslint 'src/**/*.{ts,js}' --fix",
    "db:migrate": "npm run build && sequelize-cli db:migrate --config dist/config/config.js",
    "db:rollback": "npm run build && sequelize-cli db:migrate:undo --config dist/config/config.js",
    "db:seeds": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/seeders/run_seeds.ts",
    "db:seed": "npm run build && sequelize-cli db:seed::all --config dist/config/config.js",
    "g:migration": "npx sequelize-cli migration:generate --name",
    "routes": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/scripts/routes.ts",
    "signer:sui": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/scripts/load_sui_keypair_signer.ts",
    "signer:solana": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/scripts/load_solana_keypair_signer.ts",
    "signer:base": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/scripts/load_base_keypair_signer.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@mysten/sui": "^1.21.0",
    "@sinclair/typebox": "^0.34.14",
    "@solana/web3.js": "^1.98.0",
    "@types/jsonwebtoken": "^9.0.7",
    "axios": "^1.7.9",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.7",
    "esm": "^3.2.25",
    "faker": "^6.6.6",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "fastify-postgres": "^3.6.0",
    "fishery": "^2.2.2",
    "jsonapi-serializer": "^3.6.9",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "sequelize": "^6.37.5",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/axios": "^0.14.4",
    "@types/jest": "^29.5.14",
    "@types/jsonapi-serializer": "^3.6.8",
    "@types/node": "^22.10.7",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "json-schema-to-typescript": "^15.0.4",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
